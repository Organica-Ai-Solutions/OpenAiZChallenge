# üîê Environment Variables - OpenAI Z Challenge
# Copy this file to .env and fill in your actual API keys

# ====== API KEYS ======
# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps API Key (get from https://console.cloud.google.com/)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
NEXT_PUBLIC_GOOGLE_MAPS_KEY=your_google_maps_api_key_here

# Mapbox Access Token (get from https://account.mapbox.com/access-tokens/)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# ====== API URLS ======
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_FALLBACK_API_URL=http://localhost:8003
NEXT_PUBLIC_IKRP_API_URL=http://localhost:8001

# ====== OTHER CONFIG ======
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
ENVIRONMENT=development
LOG_LEVEL=info
PYTHONPATH=/app

# ====== KAFKA & REDIS ======
REDIS_URL=redis://redis:6379
KAFKA_BOOTSTRAP_SERVERS=kafka:29092
KAFKA_CLIENT_ID=nis-protocol 