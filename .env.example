# OpenAI API Key for NIS Protocol GPT Integration
OPENAI_API_KEY='open ai key'

# Secret key
SECRET_KEY="secret key"

# Earthdata Token
EARTHDATA_TOKEN="earth token"

# Copernicus Data Space Ecosystem credentials
SENTINEL_USERNAME="sentinel user"
SENTINEL_PASSWORD="sentinel password"

# NASA Earthdata Login credentials
LIDAR_USERNAME="lidar username"
LIDAR_PASSWORD="lidar password"

# Data directories
LIDAR_DATA_DIR=./data/lidar
OUTPUT_DIR=./outputs

# API Configuration
API_HOST=localhost
API_PORT=8000

# NASA Earthdata API Configuration
EARTHDATA_API_URL=https://cmr.earthdata.nasa.gov
EARTHDATA_COLLECTION_ID=C1940468264-LPCLOUD

# Copernicus Data Space Ecosystem Configuration
CDSE_CLIENT_ID=cdse-public
CDSE_AUTH_URL=https://identity.dataspace.copernicus.eu/auth/realms/CDSE/protocol/openid-connect/token
CDSE_STAC_URL=https://catalogue.dataspace.copernicus.eu/stac

# Backend/Frontend ports
BACKEND_PORT=8000
FRONTEND_PORT=3000
HOST=0.0.0.0

# NIS Protocol settings
ENVIRONMENT=development
LOG_LEVEL=INFO
USE_REAL_DATA=true
DISABLE_MOCK_RESPONSES=true
ENABLE_MOCK_MODE=true
PROCESSING_MODE=local   
REDIS_HOST=nis-redis
REDIS_PORT=6379
REDIS_DB=0 